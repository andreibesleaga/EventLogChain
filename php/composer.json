{
    "name": "andreibesleaga/event-log-chain",
    "description": "PHP client for interacting with EventLogChain contracts",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrei Besleaga"
        }
    ],
    "require": {
        "php": ">=7.4",
        "web3p/web3.php": "^0.3",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
